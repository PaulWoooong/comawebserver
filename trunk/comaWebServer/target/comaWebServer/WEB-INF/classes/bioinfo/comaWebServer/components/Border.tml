<html xmlns:t="http://tapestry.apache.org/schema/tapestry_5_0_0.xsd">
  <head>
    <title>COMA Web Server: ${pageTitle}</title>
  </head>
  <body>
      	<div id="Container">

			
			<div>
	            <a href="http://www.ibt.lt/en/laboratories/bioinfo_en.html" id="Logo" target="_blank">
	            	<img src="${asset:context:/assets/comalogo.jpg}" style="width:150; height:150;"/>
	            </a>
	            <div id="Banner"><t:Banner/></div>
	            
	            <div id="TopMenu"><t:TopMenuClient/></div>
			</div>
			
            <div class="clear"></div>

            <div id="LeftMenu">
            	<t:security.ifloggedin>
	                <t:security.ifrole role="ROLE_ADMIN">
                           <t:LeftMenuAdmin/>
	                </t:security.ifrole>
                <t:parameter name="else">
                    <t:LeftMenuClient/>
                </t:parameter>
                </t:security.ifloggedin>
            </div>

            <div id="Body">
                <t:body/>
            </div>

            <div class="clear"></div><!--

            <div id="Footer">
				Footer
            </div>

        --></div>
  </body>
</html>
